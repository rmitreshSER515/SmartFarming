        <rdfs:range rdf:resource="http://example.org/smart-farming#Crop"/>
        <rdfs:comment>The crop recommended as the next crop in the rotation.</rdfs:comment>
        <rdfs:label>recommendsCrop</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://example.org/smart-farming#recommendsFertilizerProduct -->

    <owl:ObjectProperty rdf:about="http://example.org/smart-farming#recommendsFertilizerProduct">
        <rdfs:domain rdf:resource="http://example.org/smart-farming#FertilizerRecommendation"/>
        <rdfs:range rdf:resource="http://example.org/smart-farming#FertilizerProduct"/>
        <rdfs:comment>
      Fertilizer product recommended to be applied.
    </rdfs:comment>
        <rdfs:label>recommendsFertilizerProduct</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://example.org/smart-farming#riskForPest -->

    <owl:ObjectProperty rdf:about="http://example.org/smart-farming#riskForPest">

<?xml version="1.0"?>
<rdf:RDF
    xmlns="http://example.org/smart-farming#"
    xml:base="http://example.org/smart-farming"
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
    xmlns:sosa="http://www.w3.org/ns/sosa/">

  <!-- Ontology header -->
  <owl:Ontology rdf:about="http://example.org/smart-farming">
    <rdfs:label>Smart Farming – Crop Rotation, Fertilizer, Pest Risk Ontology</rdfs:label>
  </owl:Ontology>


  <!-- ========================================================= -->
  <!--  Core domain classes                                      -->
  <!-- ========================================================= -->

  <!-- Plot / field -->
  <owl:Class rdf:about="#Plot">
    <rdfs:label>Plot</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/sosa/FeatureOfInterest"/>
    <rdfs:comment>An agricultural plot or field where crops are cultivated.</rdfs:comment>
  </owl:Class>

  <!-- Crop types -->
  <owl:Class rdf:about="#Crop">
    <rdfs:label>Crop</rdfs:label>
    <rdfs:comment>A crop species or variety cultivated on a plot.</rdfs:comment>
  </owl:Class>

  <owl:Class rdf:about="#CerealCrop">
    <rdfs:subClassOf rdf:resource="#Crop"/>
    <rdfs:label>CerealCrop</rdfs:label>
    <rdfs:comment>Cereal crops such as corn, wheat, barley.</rdfs:comment>
  </owl:Class>

  <owl:Class rdf:about="#LegumeCrop">
    <rdfs:subClassOf rdf:resource="#Crop"/>
    <rdfs:label>LegumeCrop</rdfs:label>
    <rdfs:comment>Legume crops such as soybean, pea, bean.</rdfs:comment>
  </owl:Class>

  <!-- Treatments (fertilizer/management packages) -->
  <owl:Class rdf:about="#Treatment">
    <rdfs:label>Treatment</rdfs:label>
    <rdfs:comment>A management or fertilizer treatment applied to a plot.</rdfs:comment>
  </owl:Class>

  <!-- Generic record aligned with SOSA Observation -->
  <owl:Class rdf:about="#Record">
    <rdfs:label>Record</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/sosa/Observation"/>
    <rdfs:comment>Generic record of soil, yield, or weather for a plot and year.</rdfs:comment>
  </owl:Class>

  <!-- Specific record types -->
  <owl:Class rdf:about="#YieldRecord">
    <rdfs:label>YieldRecord</rdfs:label>
    <rdfs:subClassOf rdf:resource="#Record"/>
    <rdfs:comment>Record of crop yield for a plot and year.</rdfs:comment>
  </owl:Class>

  <owl:Class rdf:about="#SoilMeasurement">
    <rdfs:label>SoilMeasurement</rdfs:label>
    <rdfs:subClassOf rdf:resource="#Record"/>
    <rdfs:comment>Record of soil nutrient and property measurements.</rdfs:comment>
  </owl:Class>

  <owl:Class rdf:about="#WeatherSummary">
    <rdfs:label>WeatherSummary</rdfs:label>
    <rdfs:subClassOf rdf:resource="#Record"/>
    <rdfs:comment>Aggregated weather/climate summary for a period.</rdfs:comment>
  </owl:Class>

  <!-- Observable properties for soil and climate -->
  <owl:Class rdf:about="#SoilProperty">
    <rdfs:label>SoilProperty</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/sosa/ObservableProperty"/>
    <rdfs:comment>Observable soil property (e.g., N, P, K, pH).</rdfs:comment>
  </owl:Class>

  <owl:Class rdf:about="#ClimateProperty">
    <rdfs:label>ClimateProperty</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/sosa/ObservableProperty"/>
    <rdfs:comment>Observable climate property (e.g., rainfall, temperature).</rdfs:comment>
  </owl:Class>


  <!-- ========================================================= -->
  <!--  Core domain object properties                            -->
  <!-- ========================================================= -->

  <!-- Record -> Plot -->
  <owl:ObjectProperty rdf:about="#aboutPlot">
    <rdfs:label>aboutPlot</rdfs:label>
    <rdfs:comment>Plot to which the record refers.</rdfs:comment>
    <rdfs:domain rdf:resource="#Record"/>
    <rdfs:range rdf:resource="#Plot"/>
  </owl:ObjectProperty>

  <!-- Record -> Crop -->
  <owl:ObjectProperty rdf:about="#forCrop">
    <rdfs:label>forCrop</rdfs:label>
    <rdfs:comment>Crop to which the record refers.</rdfs:comment>
    <rdfs:domain rdf:resource="#Record"/>
    <rdfs:range rdf:resource="#Crop"/>
  </owl:ObjectProperty>

  <!-- YieldRecord -> Treatment -->
  <owl:ObjectProperty rdf:about="#withTreatment">
    <rdfs:label>withTreatment</rdfs:label>
    <rdfs:comment>Treatment applied for this yield record.</rdfs:comment>
    <rdfs:domain rdf:resource="#YieldRecord"/>
    <rdfs:range rdf:resource="#Treatment"/>
  </owl:ObjectProperty>

  <!-- Record -> ObservableProperty (SOSA) -->
  <owl:ObjectProperty rdf:about="#observesProperty">
    <rdfs:label>observesProperty</rdfs:label>
    <rdfs:comment>Observable property measured in this record.</rdfs:comment>
    <rdfs:domain rdf:resource="#Record"/>
    <rdfs:range rdf:resource="http://www.w3.org/ns/sosa/ObservableProperty"/>
  </owl:ObjectProperty>


  <!-- ========================================================= -->
  <!--  Core domain datatype properties                          -->
  <!-- ========================================================= -->

  <!-- Year of the record -->
  <owl:DatatypeProperty rdf:about="#hasYear">
    <rdfs:label>hasYear</rdfs:label>
    <rdfs:comment>Calendar year of the record.</rdfs:comment>
    <rdfs:domain rdf:resource="#Record"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#gYear"/>
  </owl:DatatypeProperty>

  <!-- Yield -->
  <owl:DatatypeProperty rdf:about="#yield_kg_per_ha">
    <rdfs:label>yield_kg_per_ha</rdfs:label>
    <rdfs:comment>Yield in kilograms per hectare.</rdfs:comment>
    <rdfs:domain rdf:resource="#YieldRecord"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
  </owl:DatatypeProperty>

  <!-- Some common soil properties (examples) -->
  <owl:DatatypeProperty rdf:about="#soil_N_mg_per_kg">
    <rdfs:label>soil_N_mg_per_kg</rdfs:label>
    <rdfs:comment>Soil nitrogen concentration (mg/kg).</rdfs:comment>
    <rdfs:domain rdf:resource="#SoilMeasurement"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="#soil_P_mg_per_kg">
    <rdfs:label>soil_P_mg_per_kg</rdfs:label>
    <rdfs:comment>Soil phosphorus concentration (mg/kg).</rdfs:comment>
    <rdfs:domain rdf:resource="#SoilMeasurement"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="#soil_K_mg_per_kg">
    <rdfs:label>soil_K_mg_per_kg</rdfs:label>
    <rdfs:comment>Soil potassium concentration (mg/kg).</rdfs:comment>
    <rdfs:domain rdf:resource="#SoilMeasurement"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="#soil_pH">
    <rdfs:label>soil_pH</rdfs:label>
    <rdfs:comment>Soil pH.</rdfs:comment>
    <rdfs:domain rdf:resource="#SoilMeasurement"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="#hasCropName">
    <rdfs:label>hasCropName</rdfs:label>
    <rdfs:comment>
      Human-readable crop name (e.g., "Glycine max L."), used for SWRL reasoning.
    </rdfs:comment>
    <rdfs:domain rdf:resource="#Crop"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="#soil_OM_pct">
    <rdfs:label>soil_OM_pct</rdfs:label>
    <rdfs:comment>Soil organic matter percentage.</rdfs:comment>
    <rdfs:domain rdf:resource="#SoilMeasurement"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="#soil_CEC">
    <rdfs:label>soil_CEC</rdfs:label>
    <rdfs:comment>Soil cation exchange capacity (meq/100g).</rdfs:comment>
    <rdfs:domain rdf:resource="#SoilMeasurement"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
  </owl:DatatypeProperty>

  <!-- ========================================================= -->
  <!--  Recommendation core (shared by all three use cases)      -->
  <!-- ========================================================= -->

  <owl:Class rdf:about="#Recommendation">
    <rdfs:label>Recommendation</rdfs:label>
    <rdfs:comment>Generic recommendation produced by the smart farming system.</rdfs:comment>
  </owl:Class>

  <!-- Use Case 1: Crop rotation recommendation -->
  <owl:Class rdf:about="#CropRotationRecommendation">
    <rdfs:subClassOf rdf:resource="#Recommendation"/>
    <rdfs:label>CropRotationRecommendation</rdfs:label>
    <rdfs:comment>
      Recommendation for the next crop to cultivate on a plot, based on soil nutrient
      status and historical yield data.
    </rdfs:comment>
  </owl:Class>

  <!-- Use Case 2: Fertilizer optimization recommendation -->
  <owl:Class rdf:about="#FertilizerRecommendation">
    <rdfs:subClassOf rdf:resource="#Recommendation"/>
    <rdfs:label>FertilizerRecommendation</rdfs:label>
    <rdfs:comment>
      Recommendation about fertilizer application (e.g., apply now, postpone, or adjust rate)
      based on soil nutrient levels and rainfall forecasts.
    </rdfs:comment>
  </owl:Class>

  <!-- Use Case 3: Pest risk mitigation recommendation -->
  <owl:Class rdf:about="#PestManagementRecommendation">
    <rdfs:subClassOf rdf:resource="#Recommendation"/>
    <rdfs:label>PestManagementRecommendation</rdfs:label>
    <rdfs:comment>
      Recommendation that proposes rotation or other actions to reduce pest recurrence risk.
    </rdfs:comment>
  </owl:Class>


  <!-- ========================================================= -->
  <!--  Use Case 2: Fertilizer optimization                      -->
  <!-- ========================================================= -->

  <owl:Class rdf:about="#FertilizerProduct">
    <rdfs:label>FertilizerProduct</rdfs:label>
    <rdfs:comment>A fertilizer product or formulation.</rdfs:comment>
  </owl:Class>

  <owl:Class rdf:about="#FertilizerApplication">
    <rdfs:label>FertilizerApplication</rdfs:label>
    <rdfs:comment>An event where fertilizer is applied to a plot.</rdfs:comment>
  </owl:Class>

  <!-- Simple rainfall forecast class (can be aligned with your weather model) -->
  <owl:Class rdf:about="#RainfallForecast">
    <rdfs:label>RainfallForecast</rdfs:label>
    <rdfs:comment>
      A forecast of rainfall for a time window relevant to fertilizer decisions.
    </rdfs:comment>
  </owl:Class>


  <!-- ========================================================= -->
  <!--  Use Case 3: Pest risk mitigation                         -->
  <!-- ========================================================= -->

  <owl:Class rdf:about="#Pest">
    <rdfs:label>Pest</rdfs:label>
    <rdfs:comment>A pest species or pest category.</rdfs:comment>
  </owl:Class>

  <owl:Class rdf:about="#PestInfestationEvent">
    <rdfs:label>PestInfestationEvent</rdfs:label>
    <rdfs:comment>
      A past infestation event of a particular pest on a plot or field.
    </rdfs:comment>
  </owl:Class>

  <owl:Class rdf:about="#PestRiskAssessment">
    <rdfs:label>PestRiskAssessment</rdfs:label>
    <rdfs:comment>
      An assessment of future pest risk, typically based on crop, soil, and climate data.
    </rdfs:comment>
  </owl:Class>


  <!-- ========================================================= -->
  <!--  Shared object properties for recommendations             -->
  <!-- ========================================================= -->

  <!-- Plot for which a recommendation is issued -->
  <owl:ObjectProperty rdf:about="#targetsPlot">
    <rdfs:label>targetsPlot</rdfs:label>
    <rdfs:comment>Plot for which a recommendation is issued.</rdfs:comment>
    <rdfs:domain rdf:resource="#Recommendation"/>
    <rdfs:range rdf:resource="#Plot"/>
  </owl:ObjectProperty>

  <!-- Soil measurement used as input for the recommendation -->
  <owl:ObjectProperty rdf:about="#usesSoilMeasurement">
    <rdfs:label>usesSoilMeasurement</rdfs:label>
    <rdfs:comment>
      SoilMeasurement record used as input when computing a recommendation.
    </rdfs:comment>
    <rdfs:domain rdf:resource="#Recommendation"/>
    <rdfs:range rdf:resource="#SoilMeasurement"/>
  </owl:ObjectProperty>

  <!-- Yield record used as input for the recommendation -->
  <owl:ObjectProperty rdf:about="#usesYieldRecord">
    <rdfs:label>usesYieldRecord</rdfs:label>
    <rdfs:comment>
      YieldRecord used as input when computing a recommendation.
    </rdfs:comment>
    <rdfs:domain rdf:resource="#Recommendation"/>
    <rdfs:range rdf:resource="#YieldRecord"/>
  </owl:ObjectProperty>

  <!-- Observed climate record used (e.g., WeatherSummary) -->
  <owl:ObjectProperty rdf:about="#usesWeatherSummary">
    <rdfs:label>usesWeatherSummary</rdfs:label>
    <rdfs:comment>
      WeatherSummary record used as input when computing a recommendation.
    </rdfs:comment>
    <rdfs:domain rdf:resource="#Recommendation"/>
    <rdfs:range rdf:resource="#WeatherSummary"/>
  </owl:ObjectProperty>

  <!-- Rainfall forecast used by fertilizer and pest recommendations -->
  <owl:ObjectProperty rdf:about="#usesRainfallForecast">
    <rdfs:label>usesRainfallForecast</rdfs:label>
    <rdfs:comment>
      RainfallForecast used as input when computing a recommendation.
    </rdfs:comment>
    <rdfs:domain rdf:resource="#Recommendation"/>
    <rdfs:range rdf:resource="#RainfallForecast"/>
  </owl:ObjectProperty>

  <!-- Recommended crop (used by crop rotation and pest mitigation) -->
  <owl:ObjectProperty rdf:about="#recommendsCrop">
    <rdfs:label>recommendsCrop</rdfs:label>
    <rdfs:comment>The crop recommended as the next crop in the rotation.</rdfs:comment>
    <rdfs:domain rdf:resource="#Recommendation"/>
    <rdfs:range rdf:resource="#Crop"/>
  </owl:ObjectProperty>


  <!-- ========================================================= -->
  <!--  Additional object properties – fertilizer (UC2)          -->
  <!-- ========================================================= -->

  <owl:ObjectProperty rdf:about="#recommendsFertilizerProduct">
    <rdfs:label>recommendsFertilizerProduct</rdfs:label>
    <rdfs:comment>
      Fertilizer product recommended to be applied.
    </rdfs:comment>
    <rdfs:domain rdf:resource="#FertilizerRecommendation"/>
    <rdfs:range rdf:resource="#FertilizerProduct"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="#createsFertilizerApplication">
    <rdfs:label>createsFertilizerApplication</rdfs:label>
    <rdfs:comment>
      Links a fertilizer recommendation to a corresponding planned application event.
    </rdfs:comment>
    <rdfs:domain rdf:resource="#FertilizerRecommendation"/>
    <rdfs:range rdf:resource="#FertilizerApplication"/>
  </owl:ObjectProperty>


  <!-- ========================================================= -->
  <!--  Additional object properties – pest risk (UC3)           -->
  <!-- ========================================================= -->

  <!-- Past infestations recorded for a plot -->
  <owl:ObjectProperty rdf:about="#hasPestInfestation">
    <rdfs:label>hasPestInfestation</rdfs:label>
    <rdfs:comment>A past pest infestation event on a plot.</rdfs:comment>
    <rdfs:domain rdf:resource="#Plot"/>
    <rdfs:range rdf:resource="#PestInfestationEvent"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="#infestationOfPest">
    <rdfs:label>infestationOfPest</rdfs:label>
    <rdfs:comment>The pest involved in a pest infestation event.</rdfs:comment>
    <rdfs:domain rdf:resource="#PestInfestationEvent"/>
    <rdfs:range rdf:resource="#Pest"/>
  </owl:ObjectProperty>

  <!-- Risk assessments attached to a plot -->
  <owl:ObjectProperty rdf:about="#hasPestRiskAssessment">
    <rdfs:label>hasPestRiskAssessment</rdfs:label>
    <rdfs:comment>Pest risk assessment associated with a plot.</rdfs:comment>
    <rdfs:domain rdf:resource="#Plot"/>
    <rdfs:range rdf:resource="#PestRiskAssessment"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="#riskForPest">
    <rdfs:label>riskForPest</rdfs:label>
    <rdfs:comment>The pest for which a risk assessment is made.</rdfs:comment>
    <rdfs:domain rdf:resource="#PestRiskAssessment"/>
    <rdfs:range rdf:resource="#Pest"/>
  </owl:ObjectProperty>

  <!-- Pest management recommendation specifics -->
  <owl:ObjectProperty rdf:about="#basedOnPestRiskAssessment">
    <rdfs:label>basedOnPestRiskAssessment</rdfs:label>
    <rdfs:comment>
      PestRiskAssessment used as input when computing a pest management recommendation.
    </rdfs:comment>
    <rdfs:domain rdf:resource="#PestManagementRecommendation"/>
    <rdfs:range rdf:resource="#PestRiskAssessment"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="#targetsPest">
    <rdfs:label>targetsPest</rdfs:label>
    <rdfs:comment>The pest whose recurrence the recommendation aims to reduce.</rdfs:comment>
    <rdfs:domain rdf:resource="#PestManagementRecommendation"/>
    <rdfs:range rdf:resource="#Pest"/>
  </owl:ObjectProperty>


  <!-- ========================================================= -->
  <!--  Shared datatype properties                               -->
  <!-- ========================================================= -->

  <!-- Target year for which the recommendation is intended -->
  <owl:DatatypeProperty rdf:about="#forYear">
    <rdfs:label>forYear</rdfs:label>
    <rdfs:comment>Year for which the recommendation is intended.</rdfs:comment>
    <rdfs:domain rdf:resource="#Recommendation"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#gYear"/>
  </owl:DatatypeProperty>

  <!-- Human-readable explanation of why the recommendation was generated -->
  <owl:DatatypeProperty rdf:about="#hasJustificationText">
    <rdfs:label>hasJustificationText</rdfs:label>
    <rdfs:comment>
      Human-readable explanation of why this recommendation was generated.
    </rdfs:comment>
    <rdfs:domain rdf:resource="#Recommendation"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>

  <!-- Numeric score (e.g., confidence, utility) -->
  <owl:DatatypeProperty rdf:about="#hasRecommendationScore">
    <rdfs:label>hasRecommendationScore</rdfs:label>
    <rdfs:comment>
      Numeric score representing confidence or utility of the recommendation.
    </rdfs:comment>
    <rdfs:domain rdf:resource="#Recommendation"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
  </owl:DatatypeProperty>


  <!-- ========================================================= -->
  <!--  Datatype properties – rainfall forecast (UC2/UC3)        -->
  <!-- ========================================================= -->

  <owl:DatatypeProperty rdf:about="#forecastRainfallAmount_mm">
    <rdfs:label>forecastRainfallAmount_mm</rdfs:label>
    <rdfs:comment>
      Total rainfall forecasted for the period, in millimeters.
    </rdfs:comment>
    <rdfs:domain rdf:resource="#RainfallForecast"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="#forecastStartTime">
    <rdfs:label>forecastStartTime</rdfs:label>
    <rdfs:domain rdf:resource="#RainfallForecast"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="#forecastEndTime">
    <rdfs:label>forecastEndTime</rdfs:label>
    <rdfs:domain rdf:resource="#RainfallForecast"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
  </owl:DatatypeProperty>


  <!-- ========================================================= -->
  <!--  Datatype properties – pest risk (UC3)                    -->
  <!-- ========================================================= -->

  <owl:DatatypeProperty rdf:about="#hasRiskScore">
    <rdfs:label>hasRiskScore</rdfs:label>
    <rdfs:comment>Numeric pest risk score.</rdfs:comment>
    <rdfs:domain rdf:resource="#PestRiskAssessment"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="#hasRiskLevel">
    <rdfs:label>hasRiskLevel</rdfs:label>
    <rdfs:comment>
      Categorical pest risk level, e.g., "low", "medium", "high".
    </rdfs:comment>
    <rdfs:domain rdf:resource="#PestRiskAssessment"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>


  <!-- ========================================================= -->
  <!--  Datatype properties – fertilizer recommendation (UC2)    -->
  <!-- ========================================================= -->

  <owl:DatatypeProperty rdf:about="#recommendedFertilizerAction">
    <rdfs:label>recommendedFertilizerAction</rdfs:label>
    <rdfs:comment>
      High-level action, e.g., "apply_now", "postpone", "reduce_rate".
    </rdfs:comment>
    <rdfs:domain rdf:resource="#FertilizerRecommendation"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="#recommendedApplicationDate">
    <rdfs:label>recommendedApplicationDate</rdfs:label>
    <rdfs:comment>
      Suggested date/time for fertilizer application (if applicable).
    </rdfs:comment>
    <rdfs:domain rdf:resource="#FertilizerRecommendation"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="#recommendedFertilizerRate_kg_per_ha">
    <rdfs:label>recommendedFertilizerRate_kg_per_ha</rdfs:label>
    <rdfs:comment>
      Recommended fertilizer rate in kilograms per hectare (if applicable).
    </rdfs:comment>
    <rdfs:domain rdf:resource="#FertilizerRecommendation"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
  </owl:DatatypeProperty>

</rdf:RDF>

        <rdfs:comment>
      RainfallForecast used as input when computing a recommendation.
    </rdfs:comment>
        <rdfs:label>usesRainfallForecast</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://example.org/smart-farming#usesSoilMeasurement -->

    <owl:ObjectProperty rdf:about="http://example.org/smart-farming#usesSoilMeasurement">
        <rdfs:domain rdf:resource="http://example.org/smart-farming#Recommendation"/>
        <rdfs:range rdf:resource="http://example.org/smart-farming#SoilMeasurement"/>
        <rdfs:comment>
      SoilMeasurement record used as input when computing a recommendation.
    </rdfs:comment>
        <rdfs:label>usesSoilMeasurement</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://example.org/smart-farming#usesWeatherSummary -->

    <owl:ObjectProperty rdf:about="http://example.org/smart-farming#usesWeatherSummary">
        <rdfs:domain rdf:resource="http://example.org/smart-farming#Recommendation"/>

        <rdfs:comment>
      WeatherSummary record used as input when computing a recommendation.
    </rdfs:comment>
        <rdfs:label>usesWeatherSummary</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://example.org/smart-farming#usesYieldRecord -->

    <owl:ObjectProperty rdf:about="http://example.org/smart-farming#usesYieldRecord">

        <rdfs:range rdf:resource="http://example.org/smart-farming#Pest"/>
        <rdfs:comment>The pest whose recurrence the recommendation aims to reduce.</rdfs:comment>
        <rdfs:label>targetsPest</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://example.org/smart-farming#targetsPlot -->

    <owl:ObjectProperty rdf:about="http://example.org/smart-farming#targetsPlot">
        <rdfs:domain rdf:resource="http://example.org/smart-farming#Recommendation"/>
        <rdfs:range rdf:resource="http://example.org/smart-farming#Plot"/>
        <rdfs:comment>Plot for which a recommendation is issued.</rdfs:comment>
        <rdfs:label>targetsPlot</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://example.org/smart-farming#usesRainfallForecast -->

    <owl:ObjectProperty rdf:about="http://example.org/smart-farming#usesRainfallForecast">
        <rdfs:domain rdf:resource="http://example.org/smart-farming#Recommendation"/>

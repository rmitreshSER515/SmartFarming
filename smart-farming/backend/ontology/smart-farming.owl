     -->

    


    <!-- http://example.org/smart-farming#aboutPlot -->

    <owl:ObjectProperty rdf:about="http://example.org/smart-farming#aboutPlot">
        <rdfs:domain rdf:resource="http://example.org/smart-farming#Record"/>
        <rdfs:range rdf:resource="http://example.org/smart-farming#Plot"/>
        <rdfs:comment>Plot to which the record refers.</rdfs:comment>
        <rdfs:label>aboutPlot</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://example.org/smart-farming#basedOnPestRiskAssessment -->

    <owl:ObjectProperty rdf:about="http://example.org/smart-farming#basedOnPestRiskAssessment">
        <rdfs:domain rdf:resource="http://example.org/smart-farming#PestManagementRecommendation"/>
        <rdfs:range rdf:resource="http://example.org/smart-farming#PestRiskAssessment"/>
        <rdfs:comment>
      PestRiskAssessment used as input when computing a pest management recommendation.
    </rdfs:comment>
        <rdfs:label>basedOnPestRiskAssessment</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://example.org/smart-farming#createsFertilizerApplication -->

    <owl:ObjectProperty rdf:about="http://example.org/smart-farming#createsFertilizerApplication">
        <rdfs:domain rdf:resource="http://example.org/smart-farming#FertilizerRecommendation"/>
        <rdfs:range rdf:resource="http://example.org/smart-farming#FertilizerApplication"/>
        <rdfs:comment>
      Links a fertilizer recommendation to a corresponding planned application event.
    </rdfs:comment>
        <rdfs:label>createsFertilizerApplication</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://example.org/smart-farming#forCrop -->

    <owl:ObjectProperty rdf:about="http://example.org/smart-farming#forCrop">
        <rdfs:domain rdf:resource="http://example.org/smart-farming#Record"/>
        <rdfs:range rdf:resource="http://example.org/smart-farming#Crop"/>
        <rdfs:comment>Crop to which the record refers.</rdfs:comment>
        <rdfs:label>forCrop</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://example.org/smart-farming#hasPestInfestation -->

    <owl:ObjectProperty rdf:about="http://example.org/smart-farming#hasPestInfestation">

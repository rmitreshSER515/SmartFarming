        <rdfs:range rdf:resource="http://example.org/smart-farming#Pest"/>
        <rdfs:comment>The pest for which a risk assessment is made.</rdfs:comment>
        <rdfs:label>riskForPest</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://example.org/smart-farming#targetsPest -->

    <owl:ObjectProperty rdf:about="http://example.org/smart-farming#targetsPest">

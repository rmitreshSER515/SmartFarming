        <rdfs:range rdf:resource="http://example.org/smart-farming#PestInfestationEvent"/>
        <rdfs:comment>A past pest infestation event on a plot.</rdfs:comment>
        <rdfs:label>hasPestInfestation</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://example.org/smart-farming#hasPestRiskAssessment -->

    <owl:ObjectProperty rdf:about="http://example.org/smart-farming#hasPestRiskAssessment">
        <rdfs:domain rdf:resource="http://example.org/smart-farming#Plot"/>
        <rdfs:range rdf:resource="http://example.org/smart-farming#PestRiskAssessment"/>
        <rdfs:comment>Pest risk assessment associated with a plot.</rdfs:comment>
        <rdfs:label>hasPestRiskAssessment</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://example.org/smart-farming#infestationOfPest -->

    <owl:ObjectProperty rdf:about="http://example.org/smart-farming#infestationOfPest">
        <rdfs:domain rdf:resource="http://example.org/smart-farming#PestInfestationEvent"/>
        <rdfs:range rdf:resource="http://example.org/smart-farming#Pest"/>
        <rdfs:comment>The pest involved in a pest infestation event.</rdfs:comment>
        <rdfs:label>infestationOfPest</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://example.org/smart-farming#observesProperty -->

    <owl:ObjectProperty rdf:about="http://example.org/smart-farming#observesProperty">
        <rdfs:domain rdf:resource="http://example.org/smart-farming#Record"/>
        <rdfs:range rdf:resource="http://www.w3.org/ns/sosa/ObservableProperty"/>
        <rdfs:comment>Observable property measured in this record.</rdfs:comment>
        <rdfs:label>observesProperty</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://example.org/smart-farming#recommendsCrop -->

    <owl:ObjectProperty rdf:about="http://example.org/smart-farming#recommendsCrop">

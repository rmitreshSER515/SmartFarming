        <rdfs:range rdf:resource="http://example.org/smart-farming#Treatment"/>
        <rdfs:comment>Treatment applied for this yield record.</rdfs:comment>
        <rdfs:label>withTreatment</rdfs:label>
    </owl:ObjectProperty>
    



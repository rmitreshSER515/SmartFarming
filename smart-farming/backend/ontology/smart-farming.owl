    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://example.org/smart-farming#forYear -->

    <owl:DatatypeProperty rdf:about="http://example.org/smart-farming#forYear">
        <rdfs:domain rdf:resource="http://example.org/smart-farming#Recommendation"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#gYear"/>
        <rdfs:comment>Year for which the recommendation is intended.</rdfs:comment>
        <rdfs:label>forYear</rdfs:label>
    </owl:DatatypeProperty>
    
    <!-- http://example.org/smart-farming#totalPrecip_mm -->

<owl:DatatypeProperty rdf:about="http://example.org/smart-farming#totalPrecip_mm">
    <rdfs:domain rdf:resource="http://example.org/smart-farming#WeatherSummary"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
    <rdfs:comment>
      Total annual precipitation at the plot for this WeatherSummary, in millimeters.
    </rdfs:comment>
    <rdfs:label>totalPrecip_mm</rdfs:label>
</owl:DatatypeProperty>




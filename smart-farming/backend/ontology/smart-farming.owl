    <!-- http://example.org/smart-farming#CerealCrop -->

    <owl:Class rdf:about="http://example.org/smart-farming#CerealCrop">
        <rdfs:subClassOf rdf:resource="http://example.org/smart-farming#Crop"/>
        <rdfs:comment>Cereal crops such as corn, wheat, barley.</rdfs:comment>
        <rdfs:label>CerealCrop</rdfs:label>
    </owl:Class>
    


    <!-- http://example.org/smart-farming#ClimateProperty -->

    <owl:Class rdf:about="http://example.org/smart-farming#ClimateProperty">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/sosa/ObservableProperty"/>
        <rdfs:comment>Observable climate property (e.g., rainfall, temperature).</rdfs:comment>
        <rdfs:label>ClimateProperty</rdfs:label>
    </owl:Class>
    


    <!-- http://example.org/smart-farming#Crop -->

    <owl:Class rdf:about="http://example.org/smart-farming#Crop">
        <rdfs:comment>A crop species or variety cultivated on a plot.</rdfs:comment>
        <rdfs:label>Crop</rdfs:label>
    </owl:Class>
    


    <!-- http://example.org/smart-farming#CropRotationRecommendation -->

    <owl:Class rdf:about="http://example.org/smart-farming#CropRotationRecommendation">
        <rdfs:subClassOf rdf:resource="http://example.org/smart-farming#Recommendation"/>
        <rdfs:comment>
      Recommendation for the next crop to cultivate on a plot, based on soil nutrient
      status and historical yield data.
    </rdfs:comment>
        <rdfs:label>CropRotationRecommendation</rdfs:label>
    </owl:Class>
    


    <!-- http://example.org/smart-farming#FertilizerApplication -->

    <owl:Class rdf:about="http://example.org/smart-farming#FertilizerApplication">
        <rdfs:comment>An event where fertilizer is applied to a plot.</rdfs:comment>
        <rdfs:label>FertilizerApplication</rdfs:label>
    </owl:Class>
    


    <!-- http://example.org/smart-farming#FertilizerProduct -->

    <owl:Class rdf:about="http://example.org/smart-farming#FertilizerProduct">
        <rdfs:comment>A fertilizer product or formulation.</rdfs:comment>
        <rdfs:label>FertilizerProduct</rdfs:label>
    </owl:Class>
    


    <!-- http://example.org/smart-farming#FertilizerRecommendation -->

    <owl:Class rdf:about="http://example.org/smart-farming#FertilizerRecommendation">
        <rdfs:subClassOf rdf:resource="http://example.org/smart-farming#Recommendation"/>
        <rdfs:comment>
      Recommendation about fertilizer application (e.g., apply now, postpone, or adjust rate)
      based on soil nutrient levels and rainfall forecasts.
    </rdfs:comment>
        <rdfs:label>FertilizerRecommendation</rdfs:label>

    </rdfs:comment>
        <rdfs:label>usesYieldRecord</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://example.org/smart-farming#withTreatment -->

    <owl:ObjectProperty rdf:about="http://example.org/smart-farming#withTreatment">

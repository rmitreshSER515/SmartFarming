        <rdfs:comment>Numeric pest risk score.</rdfs:comment>
        <rdfs:label>hasRiskScore</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://example.org/smart-farming#hasYear -->

    <owl:DatatypeProperty rdf:about="http://example.org/smart-farming#hasYear">
